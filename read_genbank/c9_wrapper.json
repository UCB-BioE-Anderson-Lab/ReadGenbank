{
    "id": "org.c9.function.bioinformatics.ReadGenbankParser.v1",
    "name": "ReadGenbank Parser",
    "description": "Parses a GenBank file and returns specified data, such as full sequence or features, as a JSON string.",
    "type": "function",
    "keywords": ["GenBank", "sequence parser", "bioinformatics", "biopython", "features"],
    "date_created": "2024-10-10T00:00:00Z",
    "date_last_modified": "2024-10-10T00:00:00Z",
    "inputs": [
      {
        "name": "file_path",
        "type": "string",
        "description": "Path to the GenBank file (e.g., .gb, .ape, .seq)."
      },
      {
        "name": "data_types",
        "type": "array",
        "description": "List of data types to extract (e.g., 'full_sequence', 'features')."
      },
      {
        "name": "sequence_range",
        "type": "tuple",
        "description": "Optional tuple defining the start and end positions of the sequence to extract."
      }
    ],
    "outputs": [
      {
        "type": "string",
        "description": "JSON string containing requested data, such as full sequence, features, and metadata."
      }
    ],
    "examples": [
      {
        "input": {
          "file_path": "seqs/SCU49845.gb",
          "data_types": ["full_sequence"],
          "sequence_range": [0, 30]
        },
        "output": {
          "full_sequence": "GATCCTCCATATACAACGGTATCTCCACCT"
        }
      },
      {
